<Window x:Class="Aparcamiento_Inteligente_2.EliminarClienteWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Aparcamiento_Inteligente_2"
        mc:Ignorable="d"
        Title="EliminarClienteWindow" Height="450" Width="800"
    ResizeMode="NoResize"
    ShowInTaskbar="False">
    <Window.Resources>
        <!--Botones-->
        <Style
            TargetType="{x:Type Button}">
            <Setter
                Property="Margin"
                Value="8" />
            <Setter
                Property="Width"
                Value="80" />
        </Style>
    </Window.Resources>
    <StackPanel
        Margin="16">
        <TextBlock
            Margin="16">Eliminar...</TextBlock>
        <StackPanel
            Orientation="Horizontal"
            HorizontalAlignment="Center">
            <Button
                x:Name="AceptarButton"
                Click="AceptarButton_Click"
                IsDefault="True">Aceptar</Button>
            <Button
                IsCancel="True">Cancelar</Button>
        </StackPanel>
    </StackPanel>

</Window>
